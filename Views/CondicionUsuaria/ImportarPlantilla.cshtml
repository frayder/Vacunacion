@model Highdmin.ViewModels.ImportarCondicionUsuariaViewModel
@{
    ViewData["Title"] = "Importar Condiciones Usuarias";
    Layout = "~/Views/Shared/_VerticalLayout.cshtml";
}

@await Component.InvokeAsync("ImportarPlantilla", new {
    model = Model,
    Title = ViewData["Title"],
    DownloadAction = "DescargarPlantilla",
    IndexAction = "Index",
    ImportAction = "ImportarPlantilla",
    SaveAction = "GuardarCondicionesImportadas",
    loadedCollection = "CondicionesCargadas",
    Controller = "CondicionUsuaria"
})
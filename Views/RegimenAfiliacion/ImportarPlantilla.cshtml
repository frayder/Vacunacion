@model Highdmin.ViewModels.ImportarRegimenAfiliacionViewModel
@{
    ViewData["Title"] = "Importar Regímenes de Afiliación";
    Layout = "~/Views/Shared/_VerticalLayout.cshtml";
}

@await Component.InvokeAsync("ImportarPlantilla", new {
    model = Model,
    Title = ViewData["Title"],
    DownloadAction = "DescargarPlantilla",
    IndexAction = "Index",
    ImportAction = "ImportarPlantilla",
    SaveAction = "GuardarRegimenesImportados",
    loadedCollection = "RegimenesCargados",
    Controller = "RegimenAfiliacion"
})
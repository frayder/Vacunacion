@model Highdmin.ViewModels.ImportarAseguradoraViewModel
@{
    ViewData["Title"] = "Importar Aseguradoras";
    Layout = "~/Views/Shared/_VerticalLayout.cshtml";
}

@await Component.InvokeAsync("ImportarPlantilla", new {
    model = Model,
    Title = ViewData["Title"],
    DownloadAction = "DescargarPlantilla",
    IndexAction = "Index",
    ImportAction = "ImportarPlantilla",
    SaveAction = "GuardarAseguradorasImportadas",
    loadedCollection = "AseguradorasCargadas",
    Controller = "Aseguradora"
})
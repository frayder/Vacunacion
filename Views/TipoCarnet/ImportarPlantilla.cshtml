@model Highdmin.ViewModels.ImportarTipoCarnetViewModel
@{
    ViewData["Title"] = "Importar Tipos de Carnet";
    Layout = "~/Views/Shared/_VerticalLayout.cshtml";
}

@await Component.InvokeAsync("ImportarPlantilla", new {
    model = Model,
    Title = ViewData["Title"],
    DownloadAction = "DescargarPlantilla",
    IndexAction = "Index",
    ImportAction = "ImportarPlantilla",
    SaveAction = "GuardarTiposImportados",
    loadedCollection = "TiposCarnetCargados",
    Controller = "TipoCarnet"
})